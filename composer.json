{
    "name": "digipolisgent/robo-digipolis-general",
    "description": "General Digipolis tasks for Robo Task Runner ",
    "type": "robo-tasks",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "consolidation/robo": "^1.0|^2.0",
        "grasmash/yaml-expander": "^1|^3",
        "ckr/arraymerger": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "DigipolisGent\\Robo\\Task\\General\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DigipolisGent\\Tests\\Robo\\Task\\General\\": "tests"
        }
    },
    "scripts": {
        "test": "robo --ansi test"
    },
